{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A high-performance URL shortener with analytics and QR code generation",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "postinstall": "cd client && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "nanoid": "^5.0.1",
    "lru-cache": "^10.0.0",
    "qrcode": "^1.5.3",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  },
  "keywords": ["url-shortener", "analytics", "qr-code", "mongodb"],
  "author": "Sebastian Feliciano",
  "license": "MIT"
}
